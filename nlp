                    if "Attachment A" in f[i+1]:
                        if "Sales Class" in f[i+2]:
                            pos=i+3
                            break
             if pos!=-1:
                for i in range(pos,len(f)):
                    m=str(f[i])
                    m=m.strip("\n")
                    n=m.strip(" ")
                    n=n.strip("\t")
                    ls.append(n[0:2])
                print(ls)
    else:
        for i in range(0,b):
            if "Attachment A" in f[i] and len(str(f[i]))==13:
                if "CATEGORIES" in f[i+1]:
                    if "Sales" in f[i+2]:
                        a=i+3
                        break
        for i in range(a,b):
