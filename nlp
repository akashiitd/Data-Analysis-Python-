        fhand=open(myfile,'r')
        for line in fhand:
            if "Product Categories:" in line:
                m=str(line)
                m=m.strip("\n")
                n=m.replace("Product Categories:","")
                n=n.strip(" ")
                n=n.strip("\t")
                pos=n.find(" Base")
                if pos!=-1:
                    p=n[0:pos]
                    bs.append(p)
                else:
                    if n!="":
                        bs.append(n)
                break
            elif "Product Categories" in line:
                m=str(line)
                m=m.strip("\n")
                n=m.replace("Product Categories","")
                n=n.strip(" ")
                n=n.strip("\t")
                bs.append(n)
    else:
        fhand=open(myfile,'r')
        for line in fhand:
            c+=1
            if a>0 and b>a:
                #print(a,b)
                if c>=a and c<=b:
                    if "(" in line:
                        ls.append(line)
                        for i in range(len(ls)):
                            m=str(ls[i])
                            m=m.strip("\n")
                            n=m.split(" ")
                            #print(n)
                            for j in range(len(n)):
                                if "(" in n[j] and "(excluding" not in n[j]:
                                    bs.append(n[j])

